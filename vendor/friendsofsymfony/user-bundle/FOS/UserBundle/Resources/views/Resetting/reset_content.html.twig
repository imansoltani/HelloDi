<form action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST" class="fos_user_resetting_reset">
    {{ form_label(form.first_options) }}
    {{ form_widget(form.first_options) }}
    {{ form_erorrs(form.first_options) }}

    {{ form_label(form.second_options) }}
    {{ form_widget(form.second_options) }}
