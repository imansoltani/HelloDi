{% extends 'HelloDiDiDistributorsBundle:Step:ProvMasterAdd.html.twig' %}

{% set step = '1' %}

{% block step1 %}
    <div class="widget-header">
        <h3>Choose Entity</h3>
    </div> <!-- /widget-header -->
<div class="tablediv" >
    <div class="contenttitle">
        <h4>Entities</h4>
    </div>
<div class="tab-content">
    <table style="margin-top: 20px" class="table postsend"  >
        <thead>
        <tr>
            <th>Id</th>
            <th>Entname</th>
            <th>Entvatnumber</th>
            <th>Enttel1</th>
            <th>Enttel2</th>
            <th>Entfax</th>
            <th>Entwebsite</th>
            <th>Choose</th>

        </tr>
        </thead>
        <tbody>
        {% for entiti in  pagination %}
            <tr>
                <td>{{ entiti.id }}</td>
                <td>{{ entiti.entName }}</td>
                <td>{{ entiti.entVatNumber }}</td>
                <td>{{ entiti.entTel1 }}</td>
                <td>{{ entiti.entTel2 }}</td>
                <td>{{ entiti.entFax }}</td>
                <td>{{ entiti.entWebsite }}</td>
                <td class="td-actions" >
                    <a class="btn btn-small btn-warning" href="{{ path('AddAccountProvMasterOk',{'id':entiti.id}) }}">
                        <i class="btn-icon-only icon-ok"></i>
                    </a>
                </td>

            </tr>
        {% endfor %}
        </tbody>
    </table>

    <div class="navigation">
        {{ knp_pagination_render(pagination) }}
    </div>

</div>
    </div>

{% endblock step1 %}
